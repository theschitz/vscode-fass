{
	"name": "vscode-fass",
	"displayName": "vscode-fass",
	"description": "Search for information.",
	"version": "0.0.1",
	"publisher": "theschitz",
	"repository": {
		"type": "git",
		"url": "https://github.com/theschitz"
	},
	"keywords": [
		"Fass",
		"Medication"
	],
	"author": {
		"name": "theschitz",
		"url": "https://theschitz.se"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fass.searchFassHumans",
		"onCommand:fass.searchFassAnimals",
		"onLanguage:md"
	],
	"main": "./out/extension",
	"contributes": {
		"menus": {
			"commandPalette": [
				{
					"command": "fass.searchFassHumans"
				},
				{
					"command": "fass.searchFassAnimals"
				}
			],
			"editor/context": [
				{
					"command": "fass.searchFassHumans",
					"group": "navigation"
				},
				{
					"command": "fass.searchFassAnimals",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "fass.searchFassHumans",
				"title": "FASS Humans: Search FASS Allmänhet for selected phrase."
			},
			{
				"command": "fass.searchFassAnimals",
				"title": "FASS Animals: Search FASS Djurläkemedel for selected phrase."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.6.3",
		"vscode-test": "^1.2.0"
	}
}
